{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "0.1.0",
  "name": "bootstrap-cli",
  "description": "AI-powered CLI agent focused on Java and Python testing",
  "type": "module",
  "private": true,
  "scripts": {
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.electron.json",
    "test": "bun test",
    "build": "bun run script/build.ts",
    "dev": "bun run --conditions=browser ./src/index.ts",
    "clean": "rm -rf node_modules dist dist-electron",
    "electron:build:main": "bun build ./src/electron/main.ts ./src/electron/preload.ts --outdir ./dist-electron --target=node --external electron",
    "electron:build:renderer": "vite build --config vite.electron.renderer.config.ts",
    "electron:build": "bun run electron:build:main && bun run electron:build:renderer",
    "electron:start": "bun run electron:build && electron ./dist-electron/main.js"
  },
  "bin": {
    "bootstrap": "./bin/bootstrap"
  },
  "exports": {
    "./*": "./src/*.ts"
  },
  "devDependencies": {
